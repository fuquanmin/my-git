define("baseProject/easing",function(){jQuery.extend(jQuery.easing,{def:"easeOutQuad",easeInQuint:function(t,n,e,a,r){return a*(n/=r)*n*n*n*n+e},easeOutQuint:function(t,n,e,a,r){return a*((n=n/r-1)*n*n*n*n+1)+e},easeInOutQuint:function(t,n,e,a,r){return(n/=r/2)<1?a/2*n*n*n*n*n+e:a/2*((n-=2)*n*n*n*n+2)+e},easeOutElastic:function(t,n,e,a,r){var u=1.70158,i=0,s=a;if(0==n)return e;if(1==(n/=r))return e+a;if(i||(i=.3*r),s<Math.abs(a)){s=a;var u=i/4}else var u=i/(2*Math.PI)*Math.asin(a/s);return s*Math.pow(2,-10*n)*Math.sin(2*(n*r-u)*Math.PI/i)+a+e},easeInOutElastic:function(t,n,e,a,r){var u=1.70158,i=0,s=a;if(0==n)return e;if(2==(n/=r/2))return e+a;if(i||(i=.3*r*1.5),s<Math.abs(a)){s=a;var u=i/4}else var u=i/(2*Math.PI)*Math.asin(a/s);return 1>n?-.5*s*Math.pow(2,10*(n-=1))*Math.sin(2*(n*r-u)*Math.PI/i)+e:s*Math.pow(2,-10*(n-=1))*Math.sin(2*(n*r-u)*Math.PI/i)*.5+a+e},easeInCirc:function(t,n,e,a,r){return-a*(Math.sqrt(1-(n/=r)*n)-1)+e},easeOutCirc:function(t,n,e,a,r){return a*Math.sqrt(1-(n=n/r-1)*n)+e},easeInOutCirc:function(t,n,e,a,r){return(n/=r/2)<1?-a/2*(Math.sqrt(1-n*n)-1)+e:a/2*(Math.sqrt(1-(n-=2)*n)+1)+e}})});/* @update: 2018-5-17 18:13:13 */ 
